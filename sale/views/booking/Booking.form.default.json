{
    "name": "Booking",
    "description": "",
    "access": {
        "groups": ["booking.default.user"]
    },
    "header": {
        "actions": {
            "ACTION.SAVE": [
                { "id": "SAVE_AND_CONTINUE" },
                { "id": "SAVE_AND_CLOSE" }
            ]
        }
    },
    "actions": [
        {
            "id": "action.quote",
            "label": "Revert to Quote",
            "description": "Rental units will no longer be reserved and will be available for other bookings.",
            "controller": "sale_booking_do-quote",
            "confirm": true,
            "visible": [["status", "in", ["option", "confirmed", "validated"]], ["is_from_channelmanager", "=", false]]
        },
        {
            "id": "action.quote-from-checkedout",
            "label": "Revert to Quote",
            "description": "Rental units will no longer be reserved and will be available for other bookings.",
            "controller": "sale_booking_do-quote",
            "confirm": true,
            "visible": [["status", "in", ["checkedin", "checkedout"]], ["is_from_channelmanager", "=", false], [true, "=", "object.checkedout_revert_to_quote"]],
            "params": {
                "free_rental_units": false
            }
        },
        {
            "id": "action.option",
            "label": "Set as Option",
            "description": "Rental units will be reserved but no funding will be claimed yet.",
            "controller": "sale_booking_do-option",
            "confirm": true,
            "visible": [["status", "=", "quote"], ["status_before_revert_to_quote", "is", null]]
        },
        {
            "id": "action.option_confirm",
            "label": "Confirm Booking",
            "description": "Rental units will be reserved and the invoicing plan will be set up.",
            "controller": "sale_booking_do-option-confirm",
            "confirm": true,
            "visible": [["status", "=", "quote"], ["is_price_tbc", "=", false], ["status_before_revert_to_quote", "is", null]]
        },
        {
            "id": "action.confirm",
            "label": "Confirm Booking",
            "description": "Rental units will be reserved and the invoicing plan will be set up.",
            "controller": "sale_booking_do-confirm",
            "confirm": true,
            "visible": [["status", "=", "option"], ["is_price_tbc", "=", false]]
        },
        {
            "id": "action.checkin",
            "label": "Check In",
            "description": "The host has arrived: the rental units will be marked as occupied.",
            "controller": "sale_booking_do-checkin",
            "confirm": true,
            "visible": ["status", "in", ["confirmed", "validated"]]
        },
        {
            "id": "action.checkin-from-quote",
            "label": "Back to Check In",
            "description": "The Booking status is set back to its previous checkedin status.",
            "controller": "sale_booking_do-checkin-quote",
            "confirm": true,
            "visible": [["status", "=", "quote"], ["status_before_revert_to_quote", "=", "checkedin"]]
        },
        {
            "id": "action.checkout",
            "label": "Check Out",
            "description": "The host is leaving: the rental units will be marked for cleaning.",
            "controller": "sale_booking_do-checkout",
            "confirm": true,
            "visible": ["status", "in", ["checkedin"]]
        },
        {
            "id": "action.checkout-from-quote",
            "label": "Back to Check Out",
            "description": "The Booking status is set back to its previous checkedout status.",
            "controller": "sale_booking_do-checkout-quote",
            "confirm": true,
            "visible": [["status", "=", "quote"], ["status_before_revert_to_quote", "=", "checkedout"]]
        },
        {
            "id": "action.invoice",
            "label": "Invoice",
            "description": "All additional consumptions have been added. A proforma invoice will be generated.",
            "controller": "sale_booking_do-invoice",
            "confirm": true,
            "visible": [["status", "=", "checkedout"], ["is_from_channelmanager", "=", false] ]
        },
        {
            "id": "action.invoice_direct",
            "label": "Invoice",
            "description": "Everything will be invoiced to the booking customer (no alternate payer). A proforma invoice will be generated.",
            "controller": "sale_booking_do-invoice-direct",
            "confirm": true,
            "visible": [["status", "=", "checkedout"], ["is_from_channelmanager", "=", true] ]
        },
        {
            "id": "action.invoice_instant",
            "label": "Instant invoice",
            "description": "This will complete the fundings with a manual payment (card) and fundings will be marked as paid.",
            "controller": "sale_booking_do-invoice-instant",
            "confirm": true,
            "visible": [
                [ ["status", "=", "checkedout"], ["is_from_channelmanager", "=", true] ],
                [ ["status", "=", "checkedout"], ["nb_pers", "<=", 9] ]
            ]
        },
        {
            "id": "action.invoice_back",
            "label": "Back to Check Out",
            "description": "Adaptations are still required and the proforma invoice will have to be regenerated.",
            "controller": "sale_booking_do-checkout",
            "confirm": true,
            "visible": [["is_invoiced", "=", false], ["status", "in", ["proforma", "invoiced", "debit_balance", "credit_balance", "balanced"]]]
        },
        {
            "id": "action.cancel",
            "label": "Cancel",
            "description": "The booking will be cancelled and rental units will be freed. Possible cancellation fee will be applied.",
            "controller": "sale_booking_do-cancel",
            "params": {
                "id": "object.id"
            },
            "confirm": true,
            "visible": [
                ["status", "not in", ["proforma", "invoiced", "debit_balance", "credit_balance", "balanced", "cancelled"]],
                ["is_from_channelmanager", "=", false],
                ["is_cancelled", "=", false]
            ]
        },
        {
            "id": "action.cancel-ota-without-fee",
            "label": "Cancel without fee",
            "description": "Cancel a channel manager booking without a fee.",
            "controller": "sale_booking_do-cancel-without-fee",
            "params": {
                "id": "object.id",
                "reason": "ota"
            },
            "confirm": true,
            "visible": [
                ["status", "=", "checkedout"],
                ["is_from_channelmanager", "=", true],
                ["is_cancelled", "=", true]
            ]
        },
        {
            "id": "action.release_rentalunits",
            "label": "Release rental units",
            "description": "Rental units will be released if they haven't been yet.",
            "controller": "sale_booking_release-rentalunits",
            "confirm": true,
            "visible": [["status", "=", "quote"], ["status_before_revert_to_quote", "is", null]]
        },
        {
            "id": "action.pay_instant",
            "label": "Instant payment",
            "description": "This will complete the fundings with a manual payment (card) and fundings will be marked as paid.",
            "controller": "sale_booking_do-pay-instant",
            "confirm": true,
            "visible": [ ["status", "in", ["checkedout", "proforma", "invoiced", "debit_balance", "credit_balance"]] ]
        },
        {
            "id": "action.reimbursed",
            "label": "Mark as reimbursed",
            "description": "The booking will be closed and marked as balanced (the amount already paid by the customer has been reimbursed).",
            "controller": "sale_booking_reimbursed",
            "visible": ["status", "=", "credit_balance"]
        },
        {
            "id": "action.update-status",
            "label": "Update status",
            "description": "The status of the booking will be adapted according to the status of its fundings.",
            "controller": "sale_booking_update-status",
            "visible": [ ["status", "in", ["invoiced", "debit_balance", "credit_balance"]] ]
        },
        {
            "id": "action.import_contacts",
            "label": "Import contacts",
            "description": "The contacts from the selected customer will be (re) imported to the booking.",
            "controller": "core_model_do",
            "params": {
                "entity": "sale\\booking\\Booking",
                "action": "import_contacts",
                "ids": []
            },
            "confirm": true
        },
        {
            "id": "action.clone",
            "label": "Clone",
            "description": "Clone the booking.",
            "controller": "sale_booking_clone",
            "visible": ["is_from_channelmanager", "=", false]
        },
        {
            "id": "action.apply_loyalty_points",
            "label": "Apply loyalty points",
            "description": "Apply loyalty points on booking.",
            "controller": "sale_booking_apply-loyalty-points",
            "visible": [true, "=", "env.sale.features.booking.loyalty_points"]
        },
        {
            "id": "action.unarchive",
            "label": "Unarchive",
            "description": "Unarchive the booking.",
            "controller": "sale_booking_do-unarchive",
            "visible": ["state", "=", "archive"]
        }
    ],
    "routes": [
        {
            "id": "item.booking.file",
            "label": "Booking form",
            "description": "",
            "icon": "library_books",
            "route": "/booking/object.id",
            "context": {
                "entity": "sale\\booking\\Booking",
                "view": "form.default",
                "domain": ["id", "=", "object.id"],
                "reset": true
            }
        },
        {
            "id": "item.booking.edit",
            "label": "Booked services",
            "description": "",
            "icon": "room_service",
            "route": "/booking/object.id/services"
        },
        {
            "id": "item.booking.activities-planning",
            "label": "Activities planning",
            "description": "",
            "icon": "view_week",
            "route": "/booking/object.id/activities-planning",
            "visible": [true, "=", "env.sale.features.booking.activity_schedule_table"]
        },
        {
            "id": "item.booking.weeks-activities-program",
            "label": "Weekly activities program",
            "description": "",
            "icon": "list_alt",
            "route": "/?get=sale_booking_print-booking-activity-weekly&id=object.id",
            "target": "_blank",
            "absolute": true,
            "visible": [true, "=", "env.sale.features.booking.activity_schedule_table"]
        },
        {
            "id": "item.booking.groups-activities-program",
            "label": "Grouped activities program",
            "description": "",
            "icon": "list_alt",
            "route": "/?get=sale_booking_print-booking-activity&id=object.id",
            "target": "_blank",
            "absolute": true,
            "visible": [true, "=", "env.sale.features.booking.activity"]
        },
        {
            "id": "item.booking.room-plans",
            "label": "Room plans",
            "description": "",
            "icon": "map",
            "route": "/?get=sale_booking_print-room-plans&id=object.id",
            "target": "_blank",
            "absolute": true,
            "visible": [true, "=", "env.sale.features.booking.room_plans"]
        },
        {
            "id": "item.booking.composition",
            "label": "Hosts details",
            "description": "",
            "icon": "view_list",
            "route": "/booking/object.id/composition",
            "visible": ["status", "not in", ["quote", "option"]]
        },
        {
            "id": "item.booking.composition_invite",
            "label": "Guests list invite",
            "description": "",
            "icon": "view_list",
            "route": "/booking/object.id/composition/invite",
            "visible": ["status", "in", ["confirmed", "validated"]]
        },
        {
            "id": "item.booking.contract",
            "label": "Send contract",
            "description": "",
            "icon": "drive_file_rename_outline",
            "route": "/booking/object.id/contract/object.current_contract_id",
            "visible": [["has_contract", "=", true], ["status", "=", "confirmed"]]
        },
        {
            "id": "item.booking.quote",
            "label": "Send quote",
            "description": "",
            "icon": "drive_file_rename_outline",
            "route": "/booking/object.id/quote",
            "visible": ["status", "in", ["quote"]]
        },
        {
            "id": "item.booking.option",
            "label": "Send option",
            "description": "",
            "icon": "drive_file_rename_outline",
            "route": "/booking/object.id/option",
            "visible": ["status", "in", ["option"]]
        },
        {
            "id": "item.booking.consumptions",
            "label": "Consumptions",
            "description": "",
            "icon": "calendar_today",
            "route": "/booking/object.id",
            "context": {
                "entity": "sale\\booking\\Consumption",
                "view": "list.default",
                "domain": ["booking_id", "=", "object.id"],
                "target_entity": "sale\\booking\\Booking",
                "target_view": "form.default"
            },
            "visible": ["status", "<>", "quote"]
        }
    ],
    "layout": {
        "groups": [
            {
                "sections": [
                    {
                        "rows": [
                            {
                                "columns": [
                                    {
                                        "width": "75%",
                                        "items": [
                                            {
                                                "type": "field",
                                                "label": "Reference",
                                                "value": "name",
                                                "width": "0%",
                                                "visible": false
                                            },
                                            {
                                                "type": "field",
                                                "value": "status",
                                                "visible": false
                                            },
                                            {
                                                "type": "field",
                                                "value": "has_contract",
                                                "visible": false
                                            },
                                            {
                                                "type": "field",
                                                "value": "current_contract_id",
                                                "visible": false
                                            },
                                            {
                                                "type": "field",
                                                "value": "nb_pers",
                                                "visible": false
                                            },
                                            {
                                                "type": "field",
                                                "value": "checkedout_revert_to_quote",
                                                "visible": false
                                            },
                                            {
                                                "type": "field",
                                                "value": "status_before_revert_to_quote",
                                                "visible": false
                                            },
                                            {
                                                "type": "field",
                                                "label": "Reference",
                                                "value": "display_name",
                                                "width": "50%",
                                                "widget": {
                                                    "heading": true
                                                }
                                            },
                                            {
                                                "type": "field",
                                                "value": "type_id",
                                                "width": "25%",
                                                "widget": {
                                                    "readonly": true,
                                                    "header": false
                                                }
                                            },
                                            {
                                                "type": "field",
                                                "value": "price",
                                                "width": "20%",
                                                "widget": {
                                                    "readonly": true
                                                }
                                            },
                                            {
                                                "type": "field",
                                                "value": "date_expiry",
                                                "width": "25%",
                                                "widget": {
                                                    "readonly": true
                                                },
                                                "visible": [["status", "=", "option"], ["is_noexpiry", "=", false]]
                                            },
                                            {
                                                "type": "field",
                                                "value": "is_noexpiry",
                                                "width": "50%",
                                                "visible": [["status", "=", "option"], ["is_noexpiry", "=", true]],
                                                "widget": {
                                                    "readonly": true
                                                }
                                            },
                                            {
                                                "type": "field",
                                                "value": "is_price_tbc",
                                                "width": "50%",
                                                "visible": ["is_price_tbc", "=", true],
                                                "widget": {
                                                    "readonly": true
                                                }
                                            },
                                            {
                                                "type": "field",
                                                "value": "is_from_channelmanager",
                                                "label": "channel manager",
                                                "width": "33%",
                                                "visible": [[true, "=", "env.sale.features.booking.channel_manager"]],
                                                "widget": {
                                                    "readonly": true
                                                }
                                            },
                                            {
                                                "type": "field",
                                                "value": "extref_reservation_id",
                                                "label": "Channel manager ID",
                                                "width": "33%",
                                                "visible": [[true, "=", "env.sale.features.booking.channel_manager"], ["is_from_channelmanager", "=", true]],
                                                "widget": {
                                                    "readonly": true
                                                }
                                            },
                                            {
                                                "type": "field",
                                                "value": "guarantee_id",
                                                "label": "Guarantee",
                                                "width": "33%",
                                                "visible": [[true, "=", "env.sale.features.booking.channel_manager"], ["is_from_channelmanager", "=", true]],
                                                "widget": {
                                                    "readonly": true
                                                }
                                            },
                                            {
                                                "type": "field",
                                                "value": "is_cancelled",
                                                "width": "50%",
                                                "visible": ["is_cancelled", "=", true],
                                                "widget": {
                                                    "readonly": true
                                                }
                                            }
                                        ]
                                    },
                                    {
                                        "width": "25%",
                                        "items": [
                                            {
                                                "type": "field",
                                                "value": "date_from",
                                                "width": "50%",
                                                "widget": {
                                                    "usage": "date/plain.short.day",
                                                    "readonly": true
                                                }
                                            },
                                            {
                                                "type": "field",
                                                "value": "date_to",
                                                "width": "50%",
                                                "widget": {
                                                    "usage": "date/plain.short.day",
                                                    "readonly": true
                                                }
                                            },
                                            {
                                                "type": "field",
                                                "value": "time_from",
                                                "width": "50%",
                                                "widget": {
                                                    "readonly": true
                                                }
                                            },
                                            {
                                                "type": "field",
                                                "value": "time_to",
                                                "width": "50%",
                                                "widget": {
                                                    "readonly": true
                                                }
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                ]
            },
            {
                "sections": [
                    {
                        "label": "General info",
                        "id": "section.booking_info",
                        "rows": [
                            {
                                "columns": [
                                    {
                                        "width": "50%",
                                        "items": [
                                            {
                                                "type": "field",
                                                "label": "Center",
                                                "value": "center_id",
                                                "width": "100%"
                                            },
                                            {
                                                "type": "field",
                                                "value": "center_office_id",
                                                "width": "0%",
                                                "visible": false
                                            },
                                            {
                                                "type": "field",
                                                "value": "organisation_id",
                                                "width": "0%",
                                                "visible": false
                                            },
                                            {
                                                "type": "field",
                                                "label": "Customer",
                                                "value": "customer_identity_id",
                                                "width": "100%",
                                                "widget": {
                                                    "domain": ["id", ">", 4],
                                                    "limit": 8
                                                }
                                            },
                                            {
                                                "type": "field",
                                                "value": "center_office_id",
                                                "visible": false,
                                                "width": "50%"
                                            },
                                            {
                                                "type": "field",
                                                "value": "organisation_id",
                                                "visible": false,
                                                "width": "50%"
                                            },
                                            {
                                                "type": "field",
                                                "value": "customer_id",
                                                "visible": false,
                                                "width": "50%"
                                            },
                                            {
                                                "type": "field",
                                                "value": "is_invoiced",
                                                "visible": false,
                                                "width": "50%"
                                            },
                                            {
                                                "type": "field",
                                                "value": "paid_amount",
                                                "visible": false,
                                                "width": "50%"
                                            }
                                        ]
                                    },
                                    {
                                        "width": "50%",
                                        "items": [
                                            {
                                                "type": "field",
                                                "label": "Via Tour Operator?",
                                                "value": "has_tour_operator",
                                                "width": "100%",
                                                "visible": [true, "=", "env.sale.features.booking.channel_manager"]
                                            },
                                            {
                                                "type": "field",
                                                "label": "Tour Operator",
                                                "value": "tour_operator_id",
                                                "width": "50%",
                                                "visible": [true, "=", "env.sale.features.booking.channel_manager"]
                                            },
                                            {
                                                "type": "field",
                                                "label": "Reference",
                                                "value": "tour_operator_ref",
                                                "width": "50%",
                                                "visible": [true, "=", "env.sale.features.booking.channel_manager"]
                                            }
                                        ]
                                    }
                                ]
                            },
                            {
                                "columns": [
                                    {
                                        "width": "100%",
                                        "items": [
                                            {
                                                "type": "field",
                                                "value": "tasks_ids",
                                                "width": "100%",
                                                "widget": {
                                                    "view": "list.booking",
                                                    "domain": ["visible_date", "<=", "date.this.day.first"]
                                                },
                                                "visible": [true, "=", "env.sale.features.booking.tasks_table"]
                                            },
                                            {
                                                "type": "field",
                                                "value": "description",
                                                "width": "100%",
                                                "visible": [true, "<>", "env.sale.features.booking.tasks_table"]
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "label": "Contacts",
                        "id": "section.booking_contacts",
                        "rows": [
                            {
                                "columns": [
                                    {
                                        "width": "100%",
                                        "items": [
                                            {
                                                "type": "field",
                                                "value": "contacts_ids",
                                                "width": "100%",
                                                "widget": {
                                                    "header": {
                                                        "actions": {
                                                            "ACTION.CREATE": [ {
                                                                "view": "form.create"
                                                            } ]
                                                        }
                                                    }
                                                }
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "label": "Contracts",
                        "id": "section.booking_contracts",
                        "visible": ["status", "not in", ["quote", "option"]],
                        "rows": [
                            {
                                "columns": [
                                    {
                                        "width": "100%",
                                        "items": [
                                            {
                                                "type": "field",
                                                "value": "contracts_ids",
                                                "width": "100%",
                                                "widget": {
                                                    "order": "created",
                                                    "sort": "desc",
                                                    "header": {
                                                        "selection": {
                                                            "default" : false
                                                        },
                                                        "actions": {
                                                            "ACTION.CREATE": false
                                                        }
                                                    }
                                                }
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "label": "Consumptions",
                        "id": "section.booking_consumptions",
                        "comment": "#memo - visible property has been removed to allow user to see non-freed rental units - \"visible\": [\"status\", \"<>\", \"quote\"]",
                        "rows": [
                            {
                                "columns": [
                                    {
                                        "width": "100%",
                                        "items": [
                                            {
                                                "type": "field",
                                                "value": "consumptions_ids",
                                                "width": "100%",
                                                "widget": {
                                                    "header": {
                                                        "selection": {
                                                            "default" : false
                                                        },
                                                        "actions": {
                                                            "ACTION.CREATE": false
                                                        }
                                                    }
                                                }
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "label": "Composition",
                        "id": "section.booking_composition",
                        "visible": ["status", "not in", ["quote", "option"]],
                        "rows": [
                            {
                                "columns": [
                                    {
                                        "width": "100%",
                                        "items": [
                                            {
                                                "type": "field",
                                                "value": "composition_items_ids",
                                                "width": "100%",
                                                "widget": {
                                                    "header": {
                                                        "actions": {
                                                            "ACTION.CREATE": false
                                                        }
                                                    }
                                                }
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "id": "section.meters_readings",
                        "label": "Meters Reading",
                        "visible": [ [true, "=", "sale.features.booking.consumption_meters"], ["status", "=", "checkedout"], ["is_cancelled", "=", false], [1, "=" , "object.center_office_id"] ],
                        "comment": "This tab is meant for GG only.",
                        "rows": [
                            {
                                "columns": [
                                    {
                                        "width": "100%",
                                        "items": [
                                            {
                                                "type": "field",
                                                "value": "consumptions_meter_reading_ids",
                                                "width": "100%",
